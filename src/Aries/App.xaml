<Application x:Class="Aries.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:aries="clr-namespace:Aries"
             StartupUri="AriesMain.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
               

                <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/skinviolet.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/theme.xaml"/>


                <ResourceDictionary Source="Template/BasicTemplate/TextBlockTemplate.xaml"/>
                <ResourceDictionary Source="Template/BasicTemplate/ButtonTemplate.xaml"/>


            </ResourceDictionary.MergedDictionaries>

            <aries:ColorConverter x:Key="VersionConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
